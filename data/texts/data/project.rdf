    <acdh:Project rdf:about="https://id.acdh.oeaw.ac.at/aratea-digital/project">
        <acdh:hasTitle>aratea-digital</acdh:hasTitle>
        <acdh:hasDescription>Aratea Digital collects information about astronomy in the Early Middle Ages. The main focus of the project is the Latin transmission of the so-called Aratea texts including the Latin translations and the derivative texts based on Aratus' didactic poem Phaenomena. The project presents descriptions of their (pre-13th-century) manuscripts, references to the latest editions and relevant literature.
        </acdh:hasDescription>
        <acdh:hasStartDate>2018-03-04</acdh:hasStartDate>
        <acdh:hasEndDate>2015-06-30</acdh:hasEndDate>
        <acdh:hasPrincipalInvestigator>
            <acdh:Person rdf:about="https://orcid.org/0000-0003-2388-1114">
                <acdh:hasLastName>Dobcheva</acdh:hasLastName>
                <acdh:hasFirstName>Ivana</acdh:hasFirstName>
            </acdh:Person>
        </acdh:hasPrincipalInvestigator>
    </acdh:Project>
    
